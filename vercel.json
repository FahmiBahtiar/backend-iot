{
    "version": 2,
    "buildCommand": "bun run build",
    "installCommand": "bun install",
    "framework": null,
    "builds": [
      {
        "src": "src/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }